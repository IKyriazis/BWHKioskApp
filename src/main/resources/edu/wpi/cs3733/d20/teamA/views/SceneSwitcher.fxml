<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>


<StackPane fx:id="dialogPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.d20.teamA.controllers.SceneSwitcherController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <JFXTabPane fx:id="tabPane" prefHeight="380.0" prefWidth="479.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <Tab fx:id="mapTab" text="Hospital Map">
                    <fx:include source="SimpleMap.fxml" />
                </Tab>
                <Tab fx:id="serviceTab" text="Delivery Services">
                    <fx:include source="FlowerDeliveryServiceHome.fxml" />
                </Tab>
                <Tab fx:id="employeeLoginTab" text="Employee Login">
                    <fx:include source="EmployeeArea.fxml" />
                </Tab>
            </JFXTabPane>
            <JFXButton fx:id="informationButton" onAction="#informationButtonPressed" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
